{
  "name": "well-sync",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate-env": "ts-node generate-env.ts",
    "start": "npm run generate-env && ng serve",
    "watch": "ng build --watch --configuration development",
    "build": "npm run generate-env && ng build",
    "build:prod": "npm run generate-env && ng build --configuration production",
    "serve:prod": "serve dist/well-sync/browser --single --listen 5000 --ssl-cert cert.pem --ssl-key key.pem",
    "start:prod": "npm run build:prod && npm run serve:prod",
    "test": "ng test",
    "server:notification": "node server/send-notification.js",
    "deploy:gh": "ng deploy --base-href=/WellSync/",
    "deploy:firebase": "npm run build:prod && firebase deploy"
  },
  "dependencies": {
    "@angular/common": "^19.2.8",
    "@angular/compiler": "^19.2.8",
    "@angular/core": "^19.2.8",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "^19.2.8",
    "@angular/platform-browser": "^19.2.8",
    "@angular/platform-browser-dynamic": "^19.2.8",
    "@angular/router": "^19.2.8",
    "@angular/service-worker": "^19.2.8",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/core": "latest",
    "@ionic/angular": "^8.5.4",
    "@khmyznikov/pwa-install": "^0.5.4",
    "firebase": "^11.6.1",
    "googleapis": "^148.0.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "marked": "^15.0.9",
    "rxjs": "~7.8.0",
    "swiper": "^10.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "^19.2.8",
    "@angular/compiler-cli": "^19.2.8",
    "@capacitor/cli": "latest",
    "@ionic/angular-toolkit": "latest",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.13.13",
    "angular-cli-ghpages": "^2.0.3",
    "dotenv": "^16.4.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2"
  }
}
